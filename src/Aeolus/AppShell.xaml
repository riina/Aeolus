<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Aeolus.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Aeolus"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <Tab Title="Reent">
            <ShellContent ContentTemplate="{DataTemplate local:RecentProjects}" />
        </Tab>
        <Tab Title="Projects">
            <ShellContent ContentTemplate="{DataTemplate local:ProjectDirectoryProjects}" />
        </Tab>
        <Tab Title="Folders">
            <ShellContent ContentTemplate="{DataTemplate local:ProjectDirectories}" />
        </Tab>
    </TabBar>

</Shell>
